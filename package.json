{
  "name": "saga-query",
  "version": "0.0.9",
  "description": "Data synchronization using redux-saga",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/neurosnap/saga-query.git",
  "author": "Eric Bower <me@erock.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "tsc --noEmit && ava",
    "prepare": "husky install",
    "format": "prettier --write **/*.ts",
    "build": "tsc",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@types/node": "^15.3.0",
    "ava": "^3.15.0",
    "husky": ">=6",
    "immer": "^9.0.2",
    "lint-staged": ">=10",
    "prettier": "^2.3.0",
    "redux": "^4.1.0",
    "reselect": "^4.0.0",
    "robodux": "^10.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "redux-saga": "^1.1.3",
    "redux-saga-creator": "^2.0.1"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  }
}
